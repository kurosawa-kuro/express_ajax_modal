<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>Item List</title>
</head>

<body>
    <h1>Item List</h1>

    <% items.forEach(function(item) { %>
        <p>
            <%= item.title %>
        </p>
        <% }); %>

            <form method="post">
                <label for="item">Add Item:</label>
                <input type="text" id="item" name="item">
                <button type="submit">Add</button>
            </form>
</body>

</html>